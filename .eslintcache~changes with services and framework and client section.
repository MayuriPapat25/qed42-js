wrap min-h-screen w-full px-10 py-16 bg-white overflow-hidden' \n      style={{zIndex: '1', fontFamily: 'Montserrat', maxHeight: window.innerHeight}}>\n      <h1 className='text-6xl font-semibold text-gray-500 w-full' style={{fontFamily: 'Montserrat'}}>Our Services</h1>\n      <div className='w-full flex justify-around'>\n        <div className='list-wrapper'>\n          \n            <div {...bind} className=\"list w-screen \" style={{ height: Math.max(...heights)}}>\n              {transitions.map(({ item, props: { xy, ...rest }, key }) => (\n                <a.div key={key} style={{ transform: xy.interpolate((x, y) => `translate3d(${x}px,${y}px,0)`), ...rest }}>\n                  <div style={{ backgroundImage: item.css }} />\n                </a.div>\n              ))}\n            </div>\n          \n\n        </div>\n        <div className='w-4/12 flex flex-col h-full relative'>\n          <div className='w-screen mt-4 absolute'>\n            <h3 className='text-2xl text-gray-600 font-semibold' style={{fontFamily: 'Montserrat'}}>{result}</h3>\n          </div>\n          <Service1 />\n          <Service2 />\n        </div>\n      </div>\n      {\n        // console.log('transition our services : ', transitions)\n      }\n    </div>\n  )\n}","/Users/mayuripapat/workspace/workspace/react/css/qed42-js/src/components/Banner/index.js",["102","103","104","105","106","107","108","109","110"],"import React, { useContext, useEffect, useRef, useState } from 'react';\n// import forwardSvg from '../../assets/images/new.svg';\nimport takingDigitalSvg from '../../assets/images/taking-digital.svg';\nimport forwardSvg from '../../assets/images/forward.svg';\nimport anime from 'animejs/lib/anime.es.js';\nimport NET from 'vanta/dist/vanta.net.min';\nimport * as THREE from 'three';\nimport * as VFX from 'react-vfx';\nimport gsap from 'gsap';\nimport AnimationContext from '../../contexts/animation-context';\n\nexport default function Banner() {\n\n  const [fillColor, setFillColor